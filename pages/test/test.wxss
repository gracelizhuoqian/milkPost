.test-container{
  width: 100%;
  height: 100%;
  font-weight: 400;
  padding: 100rpx 0;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.panel1,.panel2,.panel3,.panel4{
  width: 70%;
  border-radius: 5px;
  padding: 10px;
  background-color: "#abc";
  text-align: center;
  margin-top: 25rpx;
  opacity: 0.6;
  box-shadow: 0px 0px 2px #aaa;
  font-size: 28rpx;
}
.panel1,.panel3{
  background: #d4b483;
}
.panel2,.panel4{
  background: #ec9193;
}
page{
  background-color: #f1f2e3;
}
.slogan text{
  margin-bottom: 10px;
  color: #111;
  display: flex;
  flex-direction: column;
  text-align: center;
  color: #444;
}

.goNext,.showResult{
  color: #777;
  font-weight: 200;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: row;
  margin-top: 50rpx;
  animation: tada 2s .5s ease both;
}
.goNext image,.showResult image{
  height: 60rpx;
  width: 60rpx; 
}
.goNext text,.showResult{
  line-height: 32rpx;
  font-size: 32rpx;
}
.declaration,.test-tip{
  text-indent: 56rpx;
  margin: 65rpx;
  line-height: 1.5;
  font-size: 28rpx;
  font-weight: 300;
}
.showResult{
  margin-top: 100rpx;
}

@-webkit-keyframes tada{
  0%{-webkit-transform:scale(1);}
  10%,20%{-webkit-transform:scale(0.9) rotate(-3deg);}
  30%,50%,70%,90%{-webkit-transform:scale(1.2) rotate(3deg);}
  40%,60%,80%{-webkit-transform:scale(1.2) rotate(-3deg);}
  100%{-webkit-transform:scale(1) rotate(0);}}