.container{
  width: 100%;
  height: 100%;
  font-weight: 400;
}
.panel1,.panel2,.panel3,.panel4{
  width: 70%;
  border-radius: 5px;
  padding: 10px;
  background-color: "#abc";
  text-align: center;
  margin-top: 25rpx;
  opacity: 0.6;
  box-shadow: 0px 0px 2px #aaa;
  font-size: 28rpx;
}
.panel1,.panel3{
  background: #d4b483;
}
.panel2,.panel4{
  background: #ec9193;
}
page{
  background-color: #f1f2e3;
}
text{
  margin-bottom: 10px;
  color: #111;
}
.goNext{
  color: #777;
  font-weight: 200;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: row;
  margin-top: 100rpx;
}
.goNext image{
  height: 60rpx;
  width: 60rpx;
}
.goNext text{
  line-height: 32rpx;
  font-size: 32rpx;
  padding-top: 16rpx;
}